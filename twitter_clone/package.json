{
  "name": "twitter_clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:docker": "next build && next start",
    "lint:next": "next lint",
    "lint:staged": "eslint **/*.{js,jsx,ts,tsx} --fix --cache",
    "format": "prettier --check --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "format:fix": "prettier --ignore-path .gitignore --check --write ./src/**/*.{js,jsx,ts,tsx}",
    "test": "jest --passWithNoTests --color",
    "test:docker": "npm run test:dev",
    "test:watch": "jest --watchAll --coverage --passWithNoTests --color",
    "test:staged": "jest --passWithNoTests --findRelatedTests --color",
    "test:coverage": "jest --coverage --passWithNoTests --color",
    "test:cypress": "cypress open",
    "test:E2E": "cypress run",
    "husky:prepare": "cd ../ && husky install",
    "code:ci": "npm run format:fix && next lint && npm run lint:staged && npm run test:coverage && npm run test:E2E",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@tanstack/react-query": "^4.35.3",
    "@types/node": "20.4.2",
